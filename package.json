{
    "name": "hello-books",
    "version": "1.0.0",
    "description": "A system that allows efficient, cost-effective management of our library.",
    "main": "index.js",
    "scripts": {
        "lint": "eslint **/*.js",
        "lint:fix": "prettier-eslint **/**/*.{js,json} --write",
        "prettier": "prettier **/**/*.{js,json} --write",
        "test": "jest --detectOpenHandles --verbose --forceExit",
        "test:client": "jest ./client --detectOpenHandles --verbose ",
        "build:server": "babel server --copy-files --out-dir build",
        "build": "npm run build:client && npm run build:server",
        "build:client": "webpack --mode=production",
        "dev": "nodemon --exec babel-node server/index",
        "migrate": "knex migrate:latest",
        "seed": "babel-node server/seed/index.js",
        "seed:prod": "node build/seed/index",
        "make:migration": "knex migrate:make",
        "migrate:rollback": "knex migrate:rollback",
        "start": "npm run build && node build/index",
        "webpack": "webpack",
        "heroku-postbuild": "npm run migrate && npm run build && npm run seed:prod",
        "coverage": "jest --forceExit --coverage && cat ./coverage/lcov.info | coveralls"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bahdcoder/hello-books-deferral.git"
    },
    "keywords": [
        "books",
        "library"
    ],
    "author": "obi chinedu frank <frankieetchy@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/bahdcoder/hello-books-deferral/issues"
    },
    "homepage": "https://github.com/bahdcoder/hello-books-deferral#readme",
    "dependencies": {
        "@fullhuman/postcss-purgecss": "^1.2.0",
        "acorn": "^6.1.1",
