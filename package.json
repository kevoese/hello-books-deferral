        "autoprefixer": "^9.5.1",
        "axios": "^0.18.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "classnames": "^2.2.6",
        "consola": "2.6.0",
        "dotenv": "^7.0.0",
        "express": "^4.16.4",
        "express-jsend": "^0.1.0",
        "formik": "^1.5.7",
        "friendly-mail": "^1.0.0",
        "husky": "^2.2.0",
        "indicative": "^5.0.8",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.16.5",
        "lint-staged": "^8.1.5",
